{
    "name": "timiki/rpc-server-bundle",
    "type": "symfony-bundle",
    "description": "JSON-RPC server bundle for symfony",
    "keywords": [
        "rpc",
        "json-rpc",
        "symfony-bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Netesin N",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "symfony/console": "^7.0 | ^8.0",
        "symfony/dependency-injection": "^7.0 | ^8.0",
        "symfony/event-dispatcher": "^7.0 | ^8.0",
        "symfony/framework-bundle": "^7.0 | ^8.0",
        "symfony/http-foundation": "^7.0 | ^8.0",
        "symfony/http-kernel": "^7.0 | ^8.0",
        "symfony/property-access": "^7.0 | ^8.0",
        "symfony/serializer": "^7.0 | ^8.0",
        "symfony/stopwatch": "^7.0 | ^8.0",
        "timiki/rpc-common": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "^7.0 | ^8.0",
        "symfony/cache": "^7.0 | ^8.0",
        "symfony/maker-bundle": "^1.13",
        "symfony/phpunit-bridge": "^7.0 | ^8.0",
        "symfony/security-bundle": "^7.0 | ^8.0",
        "symfony/translation": "^7.0 | ^8.0",
        "symfony/validator": "^7.0 | ^8.0",
        "symfony/yaml": "^7.0 | ^8.0"
    },
    "suggest": {
        "symfony/security-bundle": "For add check security for method",
        "symfony/validator": "For add validate params for method",
        "symfony/cache": "For add cache for method"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\Bundle\\RpcServerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Timiki\\Bundle\\RpcServerBundle\\": "tests"
        },
        "classmap": [
            "dev/AppKernel.php"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
