{
    "name": "timiki/rpc-server-bundle",
    "type": "symfony-bundle",
    "description": "JSON-RPC server bundle for symfony",
    "keywords": [
        "rpc",
        "json-rpc",
        "symfony-bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Netesin N",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "php": "^7|^8",
        "ext-json": "*",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/http-foundation": "^4.3|^5.0",
        "symfony/dependency-injection": "^4.3|^5.0",
        "symfony/event-dispatcher": "^4.3|^5.0",
        "symfony/serializer": "^4.3|^5.0",
        "symfony/property-access": "^4.3|^5.0",
        "symfony/stopwatch": "^4.3|^5.0",
        "symfony/framework-bundle": "^4.3|^5.0",
        "timiki/rpc-common": "^1.0",
        "symfony/validator": "^4.3|^5.0",
        "symfony/security-core": "^4.3|^5.0",
        "symfony/console": "^4.3|^5.0",
        "doctrine/annotations": "^1.6",
        "doctrine/cache": "^1.7|^1.8"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3|^5.0",
        "symfony/yaml": "^4.3|^5.0",
        "symfony/browser-kit": "^4.3|^5.0",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/translation": "^4.3|^5.0",
        "symfony/maker-bundle": "^1.13"
    },
    "suggest": {
        "symfony/security-bundle": "For add check security for method",
        "symfony/validator": "For add validate params for method"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\Bundle\\RpcServerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Timiki\\Bundle\\RpcServerBundle\\": "tests"
        },
        "classmap": [
            "dev/AppKernel.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    }
}
