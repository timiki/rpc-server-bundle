{
    "name": "timiki/rpc-server-bundle",
    "type": "symfony-bundle",
    "description": "JSON-RPC server bundle for symfony",
    "keywords": [
        "rpc",
        "json-rpc",
        "symfony-bundle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Netesin N",
            "email": "nikita@netesin.ru"
        }
    ],
    "require": {
        "php": "^8",
        "ext-json": "*",
        "doctrine/annotations": "^1.6",
        "symfony/cache": "^5.0|^6.0",
        "symfony/console": "^5.0|^6.0",
        "symfony/dependency-injection": "^5.0|^6.0",
        "symfony/event-dispatcher": "^5.0|^6.0",
        "symfony/framework-bundle": "^5.0|^6.0",
        "symfony/http-foundation": "^5.0|^6.0",
        "symfony/http-kernel": "^5.0|^6.0",
        "symfony/property-access": "^5.0|^6.0",
        "symfony/security-core": "^5.0|^6.0",
        "symfony/serializer": "^5.0|^6.0",
        "symfony/stopwatch": "^5.0|^6.0",
        "symfony/validator": "^5.0|^6.0",
        "timiki/rpc-common": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0|^3.0",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^7.4|^9.0",
        "symfony/browser-kit": "^5.0|^6.0",
        "symfony/maker-bundle": "^1.13",
        "symfony/phpunit-bridge": "^5.0|^6.0",
        "symfony/translation": "^5.0|^6.0",
        "symfony/yaml": "^5.0|^6.0"
    },
    "suggest": {
        "symfony/security-bundle": "For add check security for method",
        "symfony/validator": "For add validate params for method"
    },
    "autoload": {
        "psr-4": {
            "Timiki\\Bundle\\RpcServerBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Timiki\\Bundle\\RpcServerBundle\\": "tests"
        },
        "classmap": [
            "dev/AppKernel.php"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
